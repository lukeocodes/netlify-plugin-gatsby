{
  "name": "netlify-plugin-gatsby",
  "version": "1.0.0",
  "description": "Netlify Build plugin gatsby - Run Gatsby seamlessly on Netlify.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && npm run test:e2e",
    "test:e2e": "cd plugin/test && ./e2e-test.sh",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ignore-path .gitignore --fix --cache --format=codeframe --max-warnings=0 \"plugin/{src,init}/**/*.js\"",
    "prettier": "prettier --ignore-path .gitignore --write --loglevel warn \"plugin/{.github,src,init}/**/*.{js,md,yml,json}\" \"*.{js,md,yml,json}\"",
    "ava": "cross-env FORCE_COLOR=1 ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/netlify-plugin-gatsby.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/netlify/netlify-plugin-gatsby/issues"
  },
  "homepage": "https://github.com/netlify/netlify-plugin-gatsby#readme",
  "devDependencies": {
    "@netlify/build": "latest",
    "ava": "^3.13.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "execa": "^5.0.0",
    "prettier": "^2.1.2",
    "release-it": "^14.0.3"
  }
}
