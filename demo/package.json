{
  "name": "netlify-gatsby-demo",
  "version": "1.0.0",
  "private": true,
  "description": "Function test",
  "author": "Matt Kane",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "HOST=0.0.0.0 gatsby develop",
    "start": "HOST=0.0.0.0 gatsby develop",
    "build": "gatsby -v && gatsby build && ls -l .cache public",
    "serve": "gatsby serve",
    "clean": "gatsby clean && rm -rf .netlify/cache .netlify/functions*",
    "build:netlify": "netlify build --offline",
    "preview": "netlify dev --offline",
    "test": "run-s build:netlify test:e2e",
    "test:e2e": "start-server-and-test preview 8888 test:jest",
    "test:jest": "jest"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.2",
    "gatsby": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@netlify/functions": "latest",
    "@netlify/ipx": "latest",
    "co-body": "^6.1.0",
    "cookie": "^0.4.1",
    "download": "^8.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "gatsby-plugin-netlify": "^4.0.0",
    "jest": "^27.0.0",
    "linkfs": "^2.1.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "path-to-regexp": "^6.0.0",
    "start-server-and-test": "^1.12.2",
    "tempy": "^1.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.15.0"
  }
}
